{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "es6": true,
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": 0,
    "jsx-quotes": ["warn", "prefer-single"],
    "no-unused-vars": ["warn", { "vars": "all", "args": "after-used" }]
  }
}
